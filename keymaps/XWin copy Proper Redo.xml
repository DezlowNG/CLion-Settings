<keymap version="1" name="XWin copy Proper Redo" parent="Default for XWin">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="BuildProject">
    <keyboard-shortcut first-keystroke="ctrl b" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EditorEnter">
    <keyboard-shortcut first-keystroke="enter" />
    <keyboard-shortcut first-keystroke="shift enter" />
  </action>
  <action id="EditorStartNewLine" />
  <action id="GotoDeclaration">
    <mouse-shortcut keystroke="control button1" />
  </action>
  <action id="ResizeToolWindowDown">
    <keyboard-shortcut first-keystroke="shift ctrl alt down" />
    <keyboard-shortcut first-keystroke="shift ctrl numpad2" />
  </action>
  <action id="org.intellij.plugins.markdown.ui.actions.styling.ToggleBoldAction" />
</keymap>